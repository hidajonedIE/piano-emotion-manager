{
  "buildCommand": "pnpm run build && npx expo export --platform web",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/$1"
    }
  ]
}